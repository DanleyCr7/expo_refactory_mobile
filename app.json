{
  "expo": {
    "name": "Refeitorio",
    "slug": "refectory_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/logo.png",
    "splash": {
      "image": "./images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ifpi.refeitorio"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
